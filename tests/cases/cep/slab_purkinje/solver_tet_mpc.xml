<?xml version="1.0" encoding="UTF-8" ?>
<svMultiPhysicsFile version="0.1">

<GeneralSimulationParameters>

  <Continue_previous_simulation> false </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 3 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 1 </Number_of_time_steps> 
  <Time_step_size> 0.01 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> 1 </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 10 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 
  <Save_results_in_folder> out_mpc </Save_results_in_folder>

  <Increment_in_saving_restart_files> 100 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 

</GeneralSimulationParameters>

<Add_mesh name="efib" > 
  <Set_mesh_as_fibers> true </Set_mesh_as_fibers> 
  <Mesh_file_path> mesh/purkinje.vtu  </Mesh_file_path>
  <Domain_file_path> mesh/purkinje.dat </Domain_file_path>
  <Add_face name = "fc_efib">
      <Mpc_nodes_file_path> mesh/purkinje_endnodes.txt </Mpc_nodes_file_path>
  </Add_face>
  <Mesh_scale_factor> 10 </Mesh_scale_factor> 
</Add_mesh>

<Add_mesh name="msh" > 
  <Mesh_file_path> ./mesh/tet3D.vtu   </Mesh_file_path>
  <Domain_file_path> ./mesh/tet3D.dat </Domain_file_path>
  <Mesh_scale_factor> 10 </Mesh_scale_factor>

  <Add_face name="face">
      <Face_file_path> ./mesh/tet3D_z0.vtp </Face_file_path>
  </Add_face>

</Add_mesh>

<Add_projection name = "fc_efib">
   <Project_from_face> face </Project_from_face>
   <Projection_tolerance> -1.0 </Projection_tolerance> 
</Add_projection>

<Add_equation type="CEP" >    
   <Min_iterations> 1 </Min_iterations>  
   <Max_iterations> 5 </Max_iterations> 
   <Tolerance> 1e-12 </Tolerance> 
  

   <Domain id="1">
     <Electrophysiology_model> TTP </Electrophysiology_model>

     <G_Na>13.19</G_Na> 
     <G_CaL>7.91e-05</G_CaL>
     <G_Kr>0.11</G_Kr>

     <Myocardial_zone> epi </Myocardial_zone>

     <Isotropic_conductivity>0.16</Isotropic_conductivity>

     <ODE_solver> RK </ODE_solver>
   </Domain>
   
   <Domain id="2">
     <Electrophysiology_model> TTP </Electrophysiology_model>

     <Myocardial_zone> pfib </Myocardial_zone>

     <ODE_solver> RK </ODE_solver>

     <Isotropic_conductivity>1.60</Isotropic_conductivity>
     <Anisotropic_conductivity>1.60</Anisotropic_conductivity>
   </Domain>

   
   <Domain id="3">
     <Electrophysiology_model> TTP </Electrophysiology_model>

     <Myocardial_zone> pfib </Myocardial_zone>

     <ODE_solver> RK </ODE_solver>

     <Isotropic_conductivity>1.60</Isotropic_conductivity>
     <Anisotropic_conductivity>1.60</Anisotropic_conductivity>

     <Stimulus type="Istim">
         <Amplitude> -52.0 </Amplitude>
         <Start_time> 0.0 </Start_time>
         <Duration> 15.0 </Duration>
         <Cycle_length> 800.0 </Cycle_length>
     </Stimulus>
   </Domain>

   <Output type="Spatial">
      <Action_potential> true </Action_potential>
   </Output>

   <LS type="GMRES">
      <Linear_algebra type="fsils" >
         <Preconditioner> fsils </Preconditioner>
      </Linear_algebra>
      <Max_iterations> 2000 </Max_iterations>
      <Tolerance> 1e-6 </Tolerance>
      <Krylov_space_dimension> 50 </Krylov_space_dimension>
   </LS>

</Add_equation>


</svMultiPhysicsFile>


